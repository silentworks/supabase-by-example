{
  "name": "express-supabase-by-example",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:css\" \"nodemon ./bin/www\"",
    "start": "node ./bin/www",
    "dev:css": "tailwindcss -w -i ./public/stylesheets/tailwind.css -o public/stylesheets/style.css",
    "build": "npm run build:css",
    "build:css": "tailwindcss -m -i ./public/stylesheets/tailwind.css -o public/stylesheets/app.css",
    "test": "playwright test",
    "s:start": "supabase start",
    "s:stop": "supabase stop"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.5.0",
    "@supabase/ssr": "^0.8.0",
    "cookie-parser": "~1.4.6",
    "daisyui": "^3.1.0",
    "debug": "~4.3.5",
    "dotenv": "^16.4.5",
    "express": "~4.19.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.18.0",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "pug": "3.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.45.1",
    "@types/tcp-port-used": "^1.0.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.4",
    "supabase": "2.64.1",
    "tailwindcss": "^3.4.4",
    "tcp-port-used": "^1.0.2"
  }
}
